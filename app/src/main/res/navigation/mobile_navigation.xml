<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">



    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.travelbuddyv2.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.travelbuddyv2.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.travelbuddyv2.MapsFragment"
        android:label="Map"
        tools:layout="@layout/fragment_maps"
        />

    <fragment
        android:id="@+id/navigation_user_profile"
        android:name="com.example.travelbuddyv2.UserProfileFragment"
        android:label="User"
        tools:layout="@layout/fragment_user_profile"
        />

</navigation>